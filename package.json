{
  "name": "slack-graphql",
  "version": "1.0.0",
  "description": "Slack GraphQL Example- Integrated with GraphQL, Express, ES6/ES7, Babel",
  "scripts": {
    "start": "npm run update && babel-node server/index.js",
    "update": "./node_modules/.bin/babel-node server/utils/updateSchema.js",
    "build-server": "./node_modules/.bin/babel ./server --out-dir ./bin && node bin/index.js",
    "clean": "rm -rf build && mkdir build",
    "lint": "eslint --ignore-path .gitignore server"
  },
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": "6.11.3",
    "npm": "5.3.0"
  },
  "keywords": [
    "graphql",
    "express",
    "es6",
    "es7",
    "babel"
  ],
  "author": "Adam Å»aczek",
  "license": "MIT",
  "devDependencies": {
    "chalk": "^1.1.3",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.16.0",
    "cross-env": "^3.1.4",
    "eslint": "^4.7.2",
    "express": "^4.15.4",
    "express-graphql": "^0.6.11",
    "file-loader": "^0.9.0",
    "graphql": "^0.10.0",
    "graphql-relay": "^0.4.4",
    "lodash": "^4.17.4"
  }
}
